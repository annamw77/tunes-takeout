<h2> Search for a New Suggestion </h2>
  <%= form_tag '/', :method => "get" do %>
    <%= text_field_tag :search_term %>
    <%= submit_tag 'Search' %>
  <% end %>

Params:
<%= params %>
<br>

<% if @pairing.present? %>
  <%= render "suggestion" %>
<% end %>

<h2> Top 20 Suggestions </h2>
<%= @top20 %>

<% @top20.each do |suggestion| %>
  <%= render "suggestion", :music_suggestion => SpotifyItem.find(suggestion.music_type,suggestion.music_id), :food_suggestion => FoodItem.find(suggestion.food_id) %>
<% end %>
